    "@types/cors": "^2.8.17",
    "@types/eslint": "^8.56.10",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.19.5",
    "@types/http-errors": "^2.0.4",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "^2.0.13",
