{
  "name": "neslo",
  "version": "0.0.0",
  "description": "Folding Sliding Doors",
  "type": "module",
  "engines": {
    "node": "18.20.3",
    "npm": "10.5.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "NODE_ENV=development node --loader ts-node/esm server.ts",
    "build:frontend": "ng build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build": "npm run build:frontend && npm run build:backend",
    "serve": "NODE_ENV=production node dist/server/server.mjs",
    "build:prod": "NODE_ENV=production ng build --prod && npm run build:backend",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "eslint . --ext .ts",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test serve http://localhost:4200 cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular/animations": "^18.0.4",
    "@angular/cdk": "^18.0.4",
